<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView android:id="@+id/profilePreviousOrders"
    android:layout_width="200dp"
    android:layout_height="125dp"
    android:layout_margin="2dp"
    app:cardCornerRadius="16dp"
    android:layout_marginStart="2dp"
    app:cardElevation="4dp"
    app:contentPadding="16dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:id="@+id/profilePreviousOrderRestaurantName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="Burger King"
                android:textSize="16sp"
                android:textColor="#550009"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/profilePreviousOrderName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                tools:text="1 Wooper"
                android:textSize="16sp"
                android:textStyle="bold"
                android:textColor="@color/black" />
        </LinearLayout>
</androidx.cardview.widget.CardView>

